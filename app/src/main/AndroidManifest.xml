<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.enterprise.mobileparking">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name="br.com.enterprise.mobileparking.app.MobileParkingApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar"
            android:usesCleartextTraffic="true">
        <activity android:name="br.com.enterprise.mobileparking.app.ui.adicionarCredito.AdicionarCreditoActivity">
        </activity>
        <activity android:name="br.com.enterprise.mobileparking.app.ui.estacionar.EstacionarActivity">
        </activity>
        <activity android:name="br.com.enterprise.mobileparking.app.ui.exibirOpcoesIniciais.OpcoesIniciaisActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="br.com.enterprise.mobileparking.app.ui.apresentarEstacionamentoVigente.EstacionamentoVigenteActivity">
        </activity>
    </application>

</manifest>